{
  "name": "my-component",
  "version": "0.0.1",
  "keywords": [
    "frontend",
    "quark",
    "web components"
  ],
  "scripts": {
    "dev": "npm run start",
    "start": "vite",
    "build": "rimraf lib && vite build --config vite.config.build.ts",
    "prepublishOnly": "pnpm run build"
  },
  "type": "module",
  "main": "./lib/index.cjs",
  "module": "./lib/index.js",
  "types": "./lib/types/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.cjs",
      "types": "./lib/types/src/index.d.ts"
    }
  },
  "dependencies": {
    "quarkc": "^1.0.4"
  },
  "devDependencies": {
    "@types/node": "^20.2.5",
    "less": "^4.1.3",
    "postcss": "^8.4.24",
    "quarkc": "^1.0.4",
    "rimraf": "^5.0.1",
    "vite": "^4.3.0"
  },
  "engines": {
    "node": ">=14.18.0"
  }
}
